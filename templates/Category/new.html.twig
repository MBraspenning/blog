{% extends 'base.html.twig' %}

{% set active = 'new' %}

{% block body %}

<h1 class="mt-4">Add category</h1>

{{ form_start(form) }}
    
    <div class="form-group">
        {{ form_label(form.name) }}
        {{ form_widget(form.name, {'attr': {'class': 'form-control'}}) }}
    </div>
    
    {{ form_widget(form.submit, {'attr': {'class': 'btn btn-primary'}}) }}
    
{{ form_end(form) }}

{% endblock %}